<w:wwml
 xmlns="http://www.w3.org/TR/REC-html40"
 xmlns:w="http://freecode.no/xml/namespaces/wwml-1.0"
 xmlns:html="http://www.w3.org/TR/REC-html40"
 >
 <w:HtmlWindow
  using="Webwidgets"
  bind="Callbacks.MyWindow"

  id="MyWindow"
  title="Address test">

  <w:Html id="Body">

   <w:CountryInput id="Country" />
   <w:AddressInput id="AddressInput" />
   <w:Address id="Address" />

   <w:Button id="Submit" title="Submit" />

  </w:Html>
 </w:HtmlWindow>
</w:wwml>
